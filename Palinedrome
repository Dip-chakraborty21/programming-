#include <iostream>
using namespace std;
 
int main()
{
    int num;
    
    cout<<"Enter a number ";
    cin >> num;
    
    
    int temp=num, reverse=0;
    
    
    while(temp>0)
    {
        reverse = reverse*10 + temp%10;
        temp = temp/10;
    }
    
    if(reverse == num)
        cout << "Palindrome number ";
    else
        cout << "Not a Palindrome number ";
    
    return 0;
}
