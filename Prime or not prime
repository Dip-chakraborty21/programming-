#include<iostream>
using namespace std;
int main()
{
    int n, i, k=0;
    
    cout <<"Enter a Number: ";
    cin>>n;
    for(i=2; i<n; i++)
    {
        if(n%i==0)
        {
            k++;
            break;
        }
    }
    if(k==0)
        cout <<"It is a Prime Number" << endl;
    else
        cout <<"It is not a Prime Number" << endl;
    
    return 0;
