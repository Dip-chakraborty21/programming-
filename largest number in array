#include<iostream>
using namespace std;
int main()
{
    int arr[100], n, large, i;
    cout<<"Enter the Size of array: ";
    cin >> n;
    cout<<"Enter  Array Elements: ";
    for(i=0; i<n; i++)
        cin >> arr[i];
    large = arr[0];
    for(i=1; i<n; i++)
    {
        if(large<arr[i])
            large = arr[i];
    }
    cout<<"Largest Number  " << large << endl;
    return 0;
}
