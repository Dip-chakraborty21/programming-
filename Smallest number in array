#include<iostream>
using namespace std;
int main()
{
    int arr[100], n, i, small;
    cout<<"Enter the Size for Array: ";
    cin >> n;
    cout << "Enter Array Elements ";
    for(i=0; i<n; i++)
        cin >> arr[i];
       small = arr[0];
    for(i=1; i<n; i++)
    {
        if(small>arr[i])
            small= arr[i];
    }
    cout<<"Smallest Number  "<< small << endl;
    
    return 0;
}
